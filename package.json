{
  "name": "ledgersmb-js",
  "version": "1.10.0-dev",
  "lockfileVersion": 2,
  "description": "LedgerSMB JS files",
  "keywords": [
    "JavaScript",
    "Dojo",
    "Toolkit",
    "Dijit",
    "lsmb"
  ],
  "maintainers": [
    {
      "name": "Erik Huelsmann"
    },
    {
      "name": "Chris Travers"
    }
  ],
  "contributors": [
    {
      "name": "John Locke"
    },
    {
      "name": "David Godfrey"
    },
    {
      "name": "Yves Lavoie"
    }
  ],
  "license": "GPL-2.0-or-later",
  "bugs": "https://github.com/ledgersmb/LedgerSMB/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ledgersmb/LedgerSMB.git"
  },
  "bundlesize": [
    {
      "path": "./UI/js/*"
    }
  ],
  "_browserslist-comment": [
      "explicitly exclude Edge <= 18, because 'and supports input-event'",
      "fails to exclude Edge 16-18"
  ],
  "browserslist": [
    "supports es6-module and supports async-functions and supports xhr2 and supports bloburls and supports css-variables and supports css3-boxsizing and supports border-radius and supports fetch and supports classlist and supports multicolumn and not edge <= 18"
  ],
  "devDependencies": {
    "@babel/eslint-parser": "7.18.2",
    "@babel/plugin-proposal-object-rest-spread": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@intlify/vue-i18n-loader": "4.2.0",
    "@vue/cli": "5.0.8",
    "@vue/cli-service": "5.0.8",
    "@vue/compiler-sfc": "3.2.37",
    "@vue/vue3-jest":"28.0.1",
    "@vue/test-utils": "2.0.2",
    "autoprefixer": "10.4.7",
    "babel-loader": "8.2.5",
    "browserslist": "4.21.1",
    "buffer": "6.0.3",
    "caniuse-lite": "1.0.30001365",
    "clean-webpack-plugin": "4.0.0",
    "compression-webpack-plugin": "10.0.0",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "css-minimizer-webpack-plugin": "4.0.0",
    "cssnano": "5.1.12",
    "depcheck": "1.4.3",
    "dojo-util": "1.17.2",
    "dojo-webpack-plugin": "3.0.4",
    "ejs-loader": "0.5.0",
    "eslint": "8.19.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-eslint": "7.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-webpack": "0.13.2",
    "eslint-plugin-amd": "1.1.0",
    "eslint-plugin-compat": "4.0.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.3",
    "eslint-plugin-json": "3.1.0",
    "eslint-plugin-markdown": "2.2.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-package-json": "0.1.4",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-snakecasejs": "2.2.0",
    "eslint-plugin-sort-dojo-dependency": "0.0.1",
    "eslint-plugin-vue": "9.2.0",
    "eslint-webpack-plugin": "3.2.0",
    "html-webpack-plugin": "5.5.0",
    "i18next-conv": "13.0.0",
    "inspectpack": "4.7.1",
    "jest": "28.1.2",
    "jest-cucumber": "3.0.2",
    "jest-tap-reporter": "1.9.0",
    "markdown-include": "0.4.3",
    "markdownlint": "0.26.0",
    "markdownlint-cli": "0.31.1",
    "mini-css-extract-plugin": "2.6.1",
    "npm-run-all": "4.1.5",
    "path-browserify": "1.0.1",
    "pinia": "^2.0.16",
    "postcss": "8.4.14",
    "postcss-loader": "7.0.0",
    "postcss-preset-env": "7.7.2",
    "postcss-reporter": "7.0.5",
    "prettier": "2.7.1",
    "stylelint": "14.9.1",
    "stylelint-config-standard": "26.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.5.0",
    "stylelint-no-browser-hacks": "1.2.1",
    "stylelint-no-nested-media": "0.1.0",
    "stylelint-no-unsupported-browser-features": "5.0.3",
    "stylelint-order": "5.0.0",
    "stylelint-webpack-plugin": "3.3.0",
    "stylelint-z-index-value-constraint": "1.2.0",
    "unix-timestamp": "1.0.2",
    "unused-webpack-plugin": "2.4.0",
    "vue": "3.2.37",
    "vue-i18n-extract-translations": "0.1.3",
    "vue-loader": "17.0.0",
    "vue-router": "4.1.2",
    "webpack": "5.73.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.10.0",
    "webpack-deduplication-plugin": "0.0.8",
    "webpack-dev-server": "4.9.2",
    "webpack-merge": "5.8.0",
    "webpack-sources": "3.2.3",
    "webpack-virtual-modules": "0.4.3",
    "yargs": "17.5.1"
  },
  "dependencies": {
    "content-disposition": "0.5.4",
    "dijit": "1.17.2",
    "dojo": "1.17.2",
    "robot3": "0.3.1",
    "vue-i18n": "9.1.10"
  },
  "homepage": "http://ledgersmb.org/",
  "scripts": {
    "analyzer": "webpack-bundle-analyzer logs/stats.json UI/js -h 0.0.0.0",
    "build //": "See https://corgibytes.com/blog/2017/04/18/npm-tips/",
    "build": "webpack --mode=production --stats errors-warnings",
    "build:dev": "webpack --progress --mode=development --stats errors-warnings",
    "lint": "npm run lint:css;npm run lint:js;npm run lint:markdown",
    "lint:css": "stylelint UI/css/*.css",
    "lint:js": "eslint --ext .js,.mjs ./",
    "lint:js:fix": "eslint --fix --ext .js,.mjs ./",
    "lint:markdown": "markdownlint --config .markdownlint.json --ignore ./node_modules .",
    "profile": "webpack --progress=profile --json > logs/stats.json",
    "readme": "webpack --mode=production --stats errors-warnings",
    "serve": "webpack --mode=development serve --progress --hot",
    "test": "jest"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env"
      ]
    ]
  },
  "eslintConfig": {
    "settings": {
      "import/resolver": "webpack"
    },
    "overrides": [
      {
        "files": [
          "package.json"
        ],
        "rules": {
          "package-json/sort-collections": "error",
            "indent": [
                "error",
                2
            ]
        }
      }
    ],
    "plugins": [
      "amd",
      "compat",
      "import",
      "markdown",
      "package-json",
      "sort-dojo-dependency",
      "vue",
      "prettier"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "ecmaVersion": 6,
      "sourceType": "module",
      "requireConfigFile": false,
      "templateSettings": {
        "evaluate": [
          "[%",
          "%]"
        ],
        "interpolate": [
          "[%",
          "%]"
        ],
        "escape": [
          "[%",
          "%]"
        ]
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "amd": true,
      "es6": true
    },
    "extends": [
      "airbnb-base/legacy",
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:markdown/recommended",
      "plugin:vue/vue3-recommended",
      "prettier"
    ],
    "ignorePatterns": [
      "dojo/",
      "dijit/",
      "node_modules/",
      "js/",
      "**/*.po*",
      "pod/**"
    ],
    "rules": {
      "camelcase": "error",
      "compat/compat": "warn",
      "consistent-return": "error",
      "curly": [
        "error",
        "all"
      ],
      "dot-notation": "error",
      "eqeqeq": "error",
      "func-names": 0,
      "global-require": "error",
      "guard-for-in": "error",
      "new-cap": 0,
      "no-alert": "error",
      "no-console": "error",
      "no-continue": 0,
      "no-else-return": "error",
      "no-lonely-if": "error",
      "no-multi-assign": "error",
      "no-multi-spaces": "error",
      "no-new-object": "error",
      "no-param-reassign": "error",
      "no-plusplus": 0,
      "no-restricted-globals": "error",
      "no-shadow": "error",
      "no-template-curly-in-string": "error",
      "no-undef": "error",
      "no-underscore-dangle": 0,
      "no-unused-expressions": "error",
      "no-unused-vars": "error",
      "no-use-before-define": "error",
      "no-useless-escape": "error",
      "no-useless-return": "error",
      "one-var": "error",
      "radix": "error",
      "spaced-comment": [
        "error",
        "always",
        {
          "block": {
            "balanced": true
          }
        }
      ],
      "vars-on-top": 0,
      "yoda": "error",
      "amd/no-unused-dependencies": "error",
      "amd/obsolete-dependencies": "error",
      "amd/sane-dependency-names": "error",
            "package-json/order-properties": [
                "error",
                [
                    "name",
                    "version",
                    "lockfileVersion",
                    "private",
                    "publishConfig",
                    "description",
                    "keywords",
                    "author",
                    "license",
                    "maintainers",
                    "contributors",
                    "bundlesize",
                    "main",
                    "browser",
                    "_browserslist-comment",
                    "browserslist",
                    "bugs",
                    "repository",
                    "files",
                    "bin",
                    "directories",
                    "man",
                    "config",
                    "dependencies",
                    "devDependencies",
                    "peerDependencies",
                    "optionalDependencies",
                    "bundledDependencies",
                    "homepage",
                    "scripts",
                    "engines",
                    "os",
                    "cpu",
                    "babel",
                    "eslintConfig",
                    "prettier",
                    "stylelint",
                    "lint-staged"
                ]
            ],
      "prettier/prettier": [
        "error",
        {
          "bracketSpacing": true,
          "printWidth": 80,
          "quote-props": [
            "error",
            "consistent"
          ],
          "semi": true,
          "singleQuote": false,
          "tabWidth": 4,
          "trailingComma": "none",
          "useTabs": false,
          "insertPragma": true,
          "proseWrap": "preserve"
        }
      ],
      "no-restricted-syntax": [
        "error",
        "SequenceExpression"
      ]
    }
  },
  "stylelint": {
    "defaultSeverity": "warning",
    "extends": "stylelint-config-standard",
    "plugins": [
      "stylelint-declaration-block-no-ignored-properties",
      "stylelint-no-browser-hacks/lib",
      "stylelint-no-nested-media",
      "stylelint-no-unsupported-browser-features",
      "stylelint-order"
    ],
    "ignoreFiles": [
      "**/*.js",
      "**/*.jsx",
      "**/*.html",
      "**/*.po*",
      "UI/**/dojo/**/*.css",
      "UI/**/dijit/**/*.css",
      "UI/**/util/**/*.css",
      "UI/js/**",
      "UI/pod/**"
    ],
    "rules": {
      "declaration-block-no-duplicate-custom-properties": null,
      "indentation": 4,
      "order/order": [
        "custom-properties",
        "declarations"
      ],
      "order/properties-alphabetical-order": true,
      "plugin/no-unsupported-browser-features": [true, {
        "ignorePartialSupport": true
      }],
      "plugin/no-browser-hacks": [true, {
        "browsers": [
            "last 2 versions",
            "not ie",
            "edge >= 18"
      ]}],
      "plugin/declaration-block-no-ignored-properties": true,
      "pitcher/no-nested-media": true,
      "selector-class-pattern": null,
      "custom-property-pattern": null,
      "selector-id-pattern": null
    }
  },
  "jest": {
    "automock": "false",
    "bail": "0",
    "cacheDirectory": "/tmp/jest_rs",
    "clearMocks": true,
    "collectCoverage": true,
    "collectCoverageFrom": "undefined",
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
     "/node_modules/"
    ],
    "coverageProvider": "babel",
    "coverageReporters": [
     "json",
     "text"
    ],
    "coverageThreshold": "undefined",
    "dependencyExtractor": "undefined",
    "errorOnDeprecated": "false",
    "fakeTimers": {
     "enableGlobally": false
    },
    "forceCoverageMatch": "[]",
    "globalSetup": "undefined",
    "globalTeardown": "undefined",
    "globals": "{}",
    "maxWorkers": "50%",
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": ["js", "json", "vue"],
    "moduleNameMapper": "{}",
    "modulePathIgnorePatterns": "[]",
    "notify": false,
    "notifyMode": "failure-change",
    "preset": "undefined",
    "projects": "undefined",
    "reporters": [
      ["jest-tap-reporter", {
        "logLevel": "INFO",
        "showInternalStackTraces": false
      }]
    ],
    "resetMocks": false,
    "resetModules": "false",
    "resolver": "undefined",
    "restoreMocks": "false",
    "rootDir": "UI",
    "roots": [
      "UI"
    ],
    "runner": "jest-runner",
    "setupFiles": "[]",
    "setupFilesAfterEnv": "[]",
    "slowTestThreshold": 5,
    "snapshotSerializers": "[]",
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": "{}",
    "testLocationInResults": false,
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "[]",
    "testResultsProcessor": "undefined",
    "testRunner": "jest-circus/runner",
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "@vue/vue3-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/",
      "\\.pnp\\.[^\\/]+$"
    ],
    "unmockedModulePathPatterns": "undefined",
    "verbose": "undefined",
    "watchPathIgnorePatterns": "[]",
    "watchman": "true"
  }
}
